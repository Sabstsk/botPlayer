{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "WEBHOOK_URL": "@webhook_url",
    "API_KEY": "@api_key",
    "NODE_ENV": "production"
  }
}
